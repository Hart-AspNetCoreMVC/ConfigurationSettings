@model Dictionary<string, string>

<!DOCTYPE html>
<html>
<head>
    <link asp-href-include="/lib/bootstrap/dist/css/*.min.css" rel="stylesheet" />
    <title>Result</title>
</head>
<body>

<table class="table table-striped table-bordered table-sm">
    @foreach (var kvp in Model)
    {
        <tr>
            <th>@kvp.Key</th>
            <td>@kvp.Value</td>
        </tr>
    }
</table>

</body>
</html>